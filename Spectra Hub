local function k(n,y,cb)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0,200,0,40)
	btn.Position = UDim2.new(0.5,-100,0,y)
	btn.BackgroundColor3 = Color3.fromRGB(120,0,180)
	btn.Text = n
	btn.TextColor3 = Color3.new(1,1,1)
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 18
	btn.AutoButtonColor = false
	btn.ZIndex = 4
	btn.Parent = j

	local uiCorner = Instance.new("UICorner")
	uiCorner.CornerRadius = UDim.new(0,6)
	uiCorner.Parent = btn

	btn.MouseEnter:Connect(function() btn.BackgroundColor3 = Color3.fromRGB(170,0,255) end)
	btn.MouseLeave:Connect(function() btn.BackgroundColor3 = Color3.fromRGB(120,0,180) end)
	btn.MouseButton1Click:Connect(cb)
end

local function l(ids)
	local rs = game:GetService("ReplicatedStorage")
	local W = rs.Remotes.Wear
	for _,v in ipairs(ids) do
		pcall(function() W:InvokeServer(v) end)
	end
end

local function m(col)
	pcall(function() game:GetService("ReplicatedStorage").Remotes.ChangeBodyColor:FireServer(col) end)
end

