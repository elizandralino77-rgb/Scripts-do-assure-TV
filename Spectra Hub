h.MouseButton1Click:Connect(function() g:Destroy() end)

local _min=false
f.MouseButton1Click:Connect(function()
	if not _min then
		i.Visible = false
		a.Size = UDim2.new(0,400,0,30)
		_min=true
	else
		i.Visible = true
		a.Size = UDim2.new(0,400,0,250)
		_min=false
	end
end)

task.spawn(function()
	local Players = game:GetService("Players")
	local pl = Players.LocalPlayer
	local Pg = pl:WaitForChild("PlayerGui")
	local sg = Instance.new("ScreenGui")
	sg.Name = "CreditMessage"
	sg.ResetOnSpawn = false
	sg.Parent = Pg
	local snd = Instance.new("Sound")
	snd.SoundId = "rbxassetid://94433663716765"
	snd.Volume = 5
	snd.Parent = sg
	snd:Play()
	local lbl = Instance.new("TextLabel")
	lbl.Parent = sg
	lbl.Size = UDim2.new(0,550,0,60)
	lbl.Position = UDim2.new(1,-570,1,-70)
	lbl.BackgroundTransparency = 1
	lbl.Text = _s{97,115,115,117,114,101,95,84,86,32,45,32,109,117,105,116,111,32,111,98,114,105,103,97,100,111,32,112,111,114,32,117,115,97,114,32,111,32,83,112,101,99,116,114,97,32,72,85,66,32,240,159,146,177,32,47,32,86,49,33,33} -- "assure_TV - muito obrigado por usar o Spectra HUB üï∑Ô∏è / V1!!"
	lbl.TextScaled = true
	lbl.Font = Enum.Font.GothamBold
	lbl.TextStrokeTransparency = 0.3
	lbl.TextTransparency = 0
	local RunService = game:GetService("RunService")
	local t0 = tick()
	local conn
	conn = RunService.RenderStepped:Connect(function()
		local t = tick() - t0
		local r = math.sin(t * 5) * 127 + 128
		local g2 = math.sin(t * 5 + 2) * 127 + 128
		local b2 = math.sin(t * 5 + 4) * 127 + 128
		lbl.TextColor3 = Color3.fromRGB(r,g2,b2)
		lbl.TextStrokeTransparency = 0.2 + math.sin(t * 3) * 0.1
	end)
	task.wait(5)
	conn:Disconnect()
	local TweenService = game:GetService("TweenService")
	local twn = TweenService:Create(lbl, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.In), {TextTransparency = 1})
	twn:Play()
	twn.Completed:Wait()
	sg:Destroy()
end)
